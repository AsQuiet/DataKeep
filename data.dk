DEF s string;
DEF ee string;

FLAG Transform 0
FLAG SpriteRenderer 1
FLAG ZombieSpell 2

@NoBody
struct Base
{
    entityID : s;
    entityC : int;
}

@Entity($Player)
struct Player -> Base
{
    @Components(Transform, SpriteRenderer)
    compList : ee;
}

@Entity($Enemy)
struct Enemy -> Base
{
    @Components(Transform, SpriteRenderer, ZombieSpell)
    compList : ee;
}