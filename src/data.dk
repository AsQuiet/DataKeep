@(Print, Init)
struct Player
{
    @(DefaultInt)
    age : int;
    @(otherTag, DefaultString)
    name : string;
    @(detail, DefaultString)
    last : string;
}

@(Enum)
struct PlayerStates
{

    Alive : string;
    Dead : string;
    @(End)
    Undead : string;
}

@(Zombie)
struct Zombie
{
    name : string;
    last : string;
}