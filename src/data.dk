
@(Include, NoBody)
struct IncludeStruct
{
}

@(Enum)
struct EntityType
{
Player : string;
Zombie : string;
@(End)
Undead : string;
}

struct MainEntity
{
@(DefaultString);
entityID : string;

entityType : EntityType;
}


@(Print, ToString)
struct Player -> MainEntity
{
@(DefaultInt)
health : int;

@(DefaultInt)	
damage : int;

@(DefaultDouble)
someFloatValue : double;
}


@(EndFile, NoBody)
struct IncludeStruct
{
}
